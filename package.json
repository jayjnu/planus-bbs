{
  "name": "planus-bbs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prepare": "npm run build",
    "start": "node ./dist/server.js",
    "build": "tsc -p .",
    "dev": "nodemon",
    "deploy": "gcloud app deploy",
    "browse": "gcloud app browse",
    "log": "gcloud app logs tail -s default"
  },
  "nodemonConfig": {
    "watch": [
      "src",
      "types"
    ],
    "ignore": [],
    "exec": "ts-node --files ./src/server.ts",
    "ext": "*.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/mongoose": "^5.5.12",
    "@types/morgan": "^1.7.36",
    "@types/node": "^12.7.1",
    "@types/ramda": "^0.26.19",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0"
  },
  "dependencies": {
    "express": "^4.17.3",
    "mongoose": "^5.6.9",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.2",
    "typescript": "^3.5.3"
  }
}
